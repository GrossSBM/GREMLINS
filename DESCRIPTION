Package: GREMLINS
Type: Package
Title: Generalized Multipartite Networks
Version: 0.2.0
Authors@R:  c(
          person(given = "Sophie", family = "Donnet", role = c("aut", "cre"), email = "sophie.donnet@inrae.fr", comment = c(ORCID = "0000-0003-4370-7316")),
